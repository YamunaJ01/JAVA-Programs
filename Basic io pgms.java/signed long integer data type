import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        long max = Long.MAX_VALUE; 
        long min = Long.MIN_VALUE; 
        System.out.println("Minimum value of signed integer is " + min);
        System.out.println("Maximum value of signed integer is " + max);
    }
}
